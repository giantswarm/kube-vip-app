{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "config": {
            "type": "object",
            "properties": {
                "address": {
                    "type": "string"
                }
            }
        },
        "env": {
            "type": "object",
            "properties": {
                "cp_enable": {
                    "type": "string"
                },
                "lb_enable": {
                    "type": "string"
                },
                "lb_port": {
                    "type": "string"
                },
                "svc_election": {
                    "type": "string"
                },
                "svc_enable": {
                    "type": "string"
                },
                "vip_arp": {
                    "type": "string"
                },
                "vip_cidr": {
                    "type": "string"
                },
                "vip_interface": {
                    "type": "string"
                },
                "vip_leaderelection": {
                    "type": "string"
                }
            }
        },
        "envFrom": {
            "type": "array"
        },
        "envValueFrom": {
            "type": "object"
        },
        "extraArgs": {
            "type": "object"
        },
        "extraLabels": {
            "type": "object"
        },
        "fullnameOverride": {
            "type": "string"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                }
            }
        },
        "imagePullSecrets": {
            "type": "array"
        },
        "nameOverride": {
            "type": "string"
        },
        "nodeSelector": {
            "type": "object"
        },
        "podAnnotations": {
            "type": "object"
        },
        "podSecurityContext": {
            "type": "object",
            "properties": {
                "fsGroup": {
                    "type": "integer"
                }
            }
        },
        "priorityClassName": {
            "type": "string"
        },
        "resources": {
            "type": "object",
            "properties": {
                "limits": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                },
                "requests": {
                    "type": "object",
                    "properties": {
                        "cpu": {
                            "type": "string"
                        },
                        "memory": {
                            "type": "string"
                        }
                    }
                }
            }
        },
        "securityContext": {
            "type": "object",
            "properties": {
                "capabilities": {
                    "type": "object",
                    "properties": {
                        "add": {
                            "type": "array",
                            "items": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "tolerations": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "effect": {
                        "type": "string"
                    },
                    "key": {
                        "type": "string"
                    },
                    "operator": {
                        "type": "string"
                    }
                }
            }
        }
    }
}
